{
  "name": "text2latex",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "next": "^14.0.3",
    "openai": "4",
    "posthog-js": "^1.104.4",
    "web": "^0.0.2"
  },
  "packageManager": "pnpm@7.25.1"
}
